{
  "name": "@ui5/webcomponents-testsuite",
  "version": "1.3.0",
  "description": "UI5 Web Components: Testsuite",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "private": false,
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "scripts": {
    "start": "npm-run-all --sequential clean build:base build:localization build:theming build:icons build:icons-business-suite build:icons-tnt prepare:main prepare:fiori dev",
    "clean": "rimraf dist && rimraf .port",
    "build:base": "cd ../base && nps build.nobundle",
    "build:localization": "cd ../localization && yarn build",
    "build:theming": "cd ../theming && yarn build",
    "build:icons": "cd ../icons && yarn build",
    "build:icons-business-suite": "cd ../icons-business-suite && yarn build",
    "build:icons-tnt": "cd ../icons-tnt && yarn build",
    "prepare:main": "cd ../main && nps prepare",
    "prepare:fiori": "cd ../fiori && nps prepare",
    "dev": "npm-run-all --parallel watch:base watch:localization watch:main watch:fiori bundle serve",
    "watch:base": "cd ../base && nps watch.nobundle",
    "watch:localization": "cd ../localization && nps watch",
    "watch:main": "cd ../main && nps watch.nobundle",
    "watch:fiori": "cd ../fiori && nps watch.nobundle",
    "bundle": "rollup --config rollup.config.js -w --environment DEV",
    "serve": "node ../tools/lib/serve/index.js --dir=dist/ --port=8080 --portStep=2 --packageName=testsuite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents.git",
    "directory": "packages/testsuite"
  },
  "dependencies": {
    "@ui5/webcomponents": "1.3.0",
    "@ui5/webcomponents-fiori": "1.3.0",
    "@ui5/webcomponents-icons": "1.3.0",
    "@ui5/webcomponents-icons-business-suite": "1.3.0",
    "@ui5/webcomponents-icons-tnt": "1.3.0"
  },
  "devDependencies": {
    "@ui5/webcomponents-tools": "1.3.0",
    "npm-run-all": "^4.1.5"
  }
}
