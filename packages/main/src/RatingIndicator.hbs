<div class="ui5-rating-indicator-root"
	role="slider"
	aria-roledescription="{{_ariaRoleDescription}}"
	aria-valuemin="0"
	aria-valuenow="{{value}}"
	aria-valuemax="{{max}}"
	aria-valuetext="{{value}} of {{max}}"
	aria-orientation="horizontal"
	aria-disabled="{{_ariaDisabled}}"
	aria-readonly="{{ariaReadonly}}"
	tabindex="{{tabIndex}}"
	@focusin="{{_onfocusin}}"
	@focusout="{{_onfocusout}}"
	@keydown="{{_onkeydown}}"
	title="{{tooltip}}"
	aria-label="{{accessibleName}}"
>
	<ul class="ui5-rating-indicator-list"
		aria-hidden="true"
		>
		{{#each _stars}}
			{{#if this.selected}}
				<li @click="{{../_onclick}}" data-ui5-value="{{this.index}}" class="ui5-rating-indicator-item ui5-rating-indicator-item-sel">
					<ui5-icon name="favorite"></ui5-icon>
				</li>
			{{else if this.halfStar}}
				<li @click="{{../_onclick}}" data-ui5-value="{{this.index}}" class="ui5-rating-indicator-item ui5-rating-indicator-item-half">
					{{#if this.isFiori}}
						{{> fioriSvg}}
					{{else}}
						{{> horizonSvg}}
					{{/if}}
					<div class="ui5-rating-indicator-half-icon-wrapper">
						<ui5-icon name="favorite" class="ui5-rating-indicator-half-icon"></ui5-icon>
					</div>
				</li>
			{{else if ../readonly}}
				<li @click="{{../_onclick}}" data-ui5-value="{{this.index}}" class="ui5-rating-indicator-item ui5-rating-indicator-readonly-item-unsel">
					{{#if this.isFiori}}
						{{> fioriSvg}}
					{{else}}
						{{> horizonSvg}}
					{{/if}}
				</li>
			{{else}}
				<li @click="{{../_onclick}}" data-ui5-value="{{this.index}}" class="ui5-rating-indicator-item ui5-rating-indicator-item-unsel">
					{{#if this.isFiori}}
						{{> fioriSvg}}
					{{else}}
						{{> horizonSvg}}
					{{/if}}
				</li>
			{{/if}}
		{{/each}}
	</ul>
</div>
